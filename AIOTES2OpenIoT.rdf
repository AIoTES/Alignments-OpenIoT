<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:iiot="http://inter-iot.eu/GOIoTP#" xmlns:exmo="http://exmo.inrialpes.fr/align/ext/1.0/#" xmlns:saref="https://w3id.org/saref#" xmlns:wgs84_pos="http://www.w3.org/2003/01/geo/wgs84_pos#" xmlns="http://www.inter-iot.eu/sripas#" xmlns:sripas="http://www.inter-iot.eu/sripas#" xmlns:time="http://www.w3.org/2006/time#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:dcelem="http://purl.org/dc/elements/1.1/" xmlns:co="http://inter-iot.eu/central" xmlns:geo-sf="http://www.opengis.net/def/sf/" xmlns:align="http://knowledgeweb.semanticweb.org/heterogeneity/alignment#" xmlns:sosa="http://www.w3.org/ns/sosa/" xmlns:saref-ps="http://ontology.tno.nl/saref/positionsensor#" xmlns:geosparql="http://www.opengis.net/ont/geosparql#" xmlns:openiot="http://openiot.eu/ontology/" xmlns:qudt="http://qudt.org/schema/qudt#" xmlns:iiotex="http://inter-iot.eu/GOIoTPex#">
      <align:Alignment>
            <dcelem:title>AIOTES2OpenIoT</dcelem:title>
            <exmo:version>1.0</exmo:version>
            <dcelem:creator>Qaiser</dcelem:creator>
            <dcelem:description>Downstream aligment between Activage and OpenIoT ontology</dcelem:description>
            <dcelem:date> Jan 24 16:21:56 GMT 2020</dcelem:date>
        <align:onto1>
            <align:Ontology rdf:about="http://inter-iot.eu/GOIoTPex#">
                <align:formalism>
                    <align:Formalism align:name="OWL2.0" align:uri="http://www.w3.org/2002/07/owl#"/>
                </align:formalism>
            </align:Ontology>
            </align:onto1>
        <align:onto2>
            <align:Ontology rdf:about="http://openiot.eu/ontology/">
                <align:formalism>
                    <align:Formalism align:name="OWL2.0" align:uri="http://www.w3.org/2002/07/owl#"/>
                </align:formalism>
            </align:Ontology>
            </align:onto2>
        <sripas:steps rdf:parseType="Literal">
            <sripas:step sripas:order="1" sripas:cell="http://www.inter-iot.eu/sripas#1_sensor"/>
      
            <sripas:step sripas:order="2" sripas:cell="http://www.inter-iot.eu/sripas#2_point"/>

            <sripas:step sripas:order="3" sripas:cell="http://www.inter-iot.eu/sripas#3_observation"/>
        </sripas:steps>
            <sripas:cellFormat>
                  <iiot:DataFormat rdf:about="http://inter-iot.eu/sripas#turtle"/>
            </sripas:cellFormat>
            <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_sensor">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
          [  a                sosa:Sensor ;
            rdf:label   sripas:node_ent;
            sosa:observes sripas:node_OBP ].
        </align:entity1>
        
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
         

            sripas:node_SNS a              sosa:Sensor ;
            openiot:category  sripas:node_ent;
            sosa:observes  sripas:node_OBP .

        </align:entity2>
        <align:relation>=</align:relation>
        
          <sripas:typings rdf:parseType="Literal">
                            <typing about="http://www.inter-iot.eu/sripas#node_ent" datatype="http://www.w3.org/2001/XMLSchema#string"/>
              </sripas:typings>
                </align:Cell>
          </align:map>
          <align:map>
          <align:Cell rdf:about="http://www.inter-iot.eu/sripas#2_point">
              <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
           [ a                sosa:Result , geosparql:Geometry, sripas:node_Blank ;
            geosparql:asWKT   sripas:node_point
          ] .
        </align:entity1>
        
              <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
        [ a               wgs84_pos:Point , sosa:Platform , sripas:node_Blank;
            geosparql:hasGeometry [ geosparql:asWKT sripas:node_point ]
          ] .
        </align:entity2>
        
            <align:relation>=</align:relation>
         <sripas:typings rdf:parseType="Literal">
                          <typing about="http://www.inter-iot.eu/sripas#node_point" datatype="http://www.opengis.net/def/sf/wktLiteral"/>
            </sripas:typings>

              </align:Cell>
        </align:map>

          <align:map>
          <align:Cell rdf:about="http://www.inter-iot.eu/sripas#3_observation">
              <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
          [ a                sosa:Observation , sosa:Result ;
            sosa:madBySensor  sripas:node_SNS;
            sosa:resultTime   sripas:node_created;
            sosa:hasSimpleResult sripas:node_result;
            sosa:observedProperty sripas:node_OBP
          ] .

        </align:entity1>
        
              <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
        
            [ a              sosa:Observation;
            sosa:madBySensor  sripas:node_SNS;
            sosa:resultTime   sripas:node_created;
            qudt:unit  sripas:node_unit;
            sosa:hasSimpleResult sripas:node_result;
            sosa:observedProperty sripas:node_OBP
          ] .
        </align:entity2>
        
            <align:relation>=</align:relation>
         <sripas:typings rdf:parseType="Literal">
                          <typing about="http://www.inter-iot.eu/sripas#node_created" datatype="http://www.w3.org/2001/XMLSchema#float"/>
                           <typing about="http://www.inter-iot.eu/sripas#node_result" datatype="http://www.w3.org/2001/XMLSchema#float"/>
                          <typing about="http://www.inter-iot.eu/sripas#node_unit" datatype="http://www.w3.org/2001/XMLSchema#string"/>
            </sripas:typings>

              </align:Cell>
        </align:map>

      </align:Alignment>
    </rdf:RDF>